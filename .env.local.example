# Supabase Settings
# Local development: run `npm run supabase:start` and use the values shown
# Staging: get from your Supabase project's STG settings > API
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:55331
NEXT_PUBLIC_SUPABASE_ANON_KEY=UPDATE_THIS_WITH_YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=UPDATE_THIS_WITH_YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_DB_PASSWORD=postgres

# Stripe Settings
# Test: get from https://dashboard.stripe.com/test/apikeys
# Staging: use your Stripe STG project API keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_STRIPE_PUBLISHABLE_KEY
STRIPE_SECRET_KEY=sk_test_YOUR_STRIPE_SECRET_KEY
STRIPE_WEBHOOK_SECRET=whsec_YOUR_STRIPE_WEBHOOK_SECRET

# Resend Settings
# Get from https://resend.com/api-keys
# For local development, a test key is fine
RESEND_API_KEY=re_test_YOUR_RESEND_API_KEY

# Site URL Settings
# Local development: http://localhost:3008
# Staging: https://stg.your-domain.com
NEXT_PUBLIC_SITE_URL=http://localhost:3008

# OpenAI settings (STT + LLM)
OPENAI_API_KEY=sk-YourOpenAIKey
